subdirs="auxprogs priv pub"
targets="libVEX.a"

defvars() {
	  echo 'libvex_objects=$(patsubst %.c,%.o,$(libvex_sources))'
	  echo "libvex=$input_base"
}

rules() {
	cat <<EOF
\$(libvex_objects): CPPFLAGS+=-I${input_base}/pub
libVEX.a: \$(libvex_objects)
	  echo \$(libvex_objects) ; ar rcs \$@ \$^
EOF
}